<html lang="en-US">
<head>
    <title>Survey Analysis</title>
</head>

<body>
<h1>Survey analysis</h1>
<h2>Name</h2>
<ul>
    <% for (var i=0; i<results[0].length; i++){ %>
        <li><%= results[0][i].name %> :  <%= results[0][i].count %></li>
    <% } %>
</ul>
<h2>Age</h2>
<ul>
    <% for (var i=0; i<results[1].length; i++){ %>
        <li><%= results[1][i].age %> :  <%= results[1][i].count %></li>
    <% } %>
</ul>
<h2>Gender</h2>
<ul>
    <% for (var i=0; i<results[2].length; i++){ %>
        <li><%= results[2][i].gender %> :  <%= results[2][i].count %></li>
    <% } %>
</ul>
<h2>Favourite Progamming Langauage:</h2>
<ul>
    <% for (var i=0; i<results[3].length; i++){ %>
        <li><%= results[3][i].fpl %> :  <%= results[3][i].count %></li>
    <% } %>
</ul>
<h2>Know how to program in</h2>
<ul>
    <% for (var i=0; i<results[4].length; i++){ %>
        <li><%= results[4][i].ik %> :  <%= results[4][i].count %></li>
    <% } %>
</ul>
<h2>Comment</h2>
<ul>
    <% for (var i=0; i<results[5].length; i++){ %>
        <li><%= results[5][i].comment %> :  <%= results[5][i].count %></li>
    <% } %>
</ul>

</body>
</html>